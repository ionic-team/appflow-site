{
  "name": "appflow-site",
  "private": true,
  "version": "0.0.1",
  "description": "Marketing Website for Appflow",
  "scripts": {
    "ci:env": "touch .env && echo PRISMIC_ENDPOINT=$PRISMIC_ENDPOINT >> .env",
    "ci": "npm run ci:env && npm run build",
    "build": "stencil build --prerender",
    "start": "stencil build --dev --watch --watchAll --serve",
    "generate": "stencil generate",
    "serve": "stencil build --dev --watch",
    "dev": "stencil build --dev --watch --serve",
    "blog": "blog src/pages/blog-page/assets/md src/pages/blog-page/assets/blog.json"
  },
  "devDependencies": {
    "@ionic-internal/ionic-ds": "2.1.4-3",
    "@ionic-internal/markdown-blog": "^1.0.7",
    "@ionic/prettier-config": "^1.0.0",
    "@stencil/core": "2.0.0-5",
    "@stencil/helmet": "^0.3.2",
    "@stencil/sass": "^1.4.0-0",
    "@stencil/store": "^1.3.0",
    "@stencil/utils": "0.0.5",
    "@types/marked": "^0.6.5",
    "@types/prismic-dom": "^2.1.1",
    "@types/prismjs": "^1.9.0",
    "@vercel/node": "^1.7.1",
    "date-fns": "^2.14.0",
    "dotenv": "^8.0.0",
    "front-matter": "^3.2.1",
    "gsap": "^3.5.0",
    "hubspot": "^2.3.13",
    "ionicons": "^5.0.1",
    "prismic-dom": "^2.2.3",
    "prismic-javascript": "^2.7.1",
    "prismjs": "^1.21.0",
    "rollup-plugin-dotenv": "^0.3.0",
    "stencil-router-v2": "^0.5.0"
  },
  "prettier": "@ionic/prettier-config",
  "author": "Ionic Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/appflow-site.git"
  },
  "bugs": {
    "url": "https://github.com/ionic-team/appflow-site"
  },
  "homepage": "https://github.com/ionic-team/appflow-site",
  "dependencies": {
    "@ionic/core": "^5.3.2",
    "marked": "^1.1.1",
    "node-fetch": "^2.6.0"
  }
}
